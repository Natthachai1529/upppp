<?xml version="1.0" ?>
<odoo noupdate="1">
    <record id="seq_billing_note" model="ir.sequence">
        <field name="name">Billing note sequence</field>
        <field name="code">billing.note</field>
        <field name="prefix">BLN-%(year)s-%(month)s-%(day)s-</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>
